{
  "name": "@kogito-tooling-examples/webapp",
  "version": "0.16.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack",
    "build:dev": "yarn run run-script-if --bool $(build-env global.build.examples) --then \"yarn run build --env dev\"",
    "build:prod": "yarn run run-script-if --bool $(build-env global.build.examples) --then \"yarn run build\"",
    "start": "webpack serve --host 0.0.0.0 --env dev"
  },
  "dependencies": {
    "@kie-tooling-core/envelope-bus": "0.16.0",
    "@kie-tooling-core/patternfly-base": "0.16.0",
    "@kogito-tooling-examples/base64png-editor": "0.16.0",
    "@kogito-tooling-examples/ping-pong-view": "0.16.0",
    "@kogito-tooling-examples/ping-pong-view-react": "0.16.0",
    "@kogito-tooling-examples/todo-list-view": "0.16.0",
    "@kogito-tooling/kie-bc-editors": "0.16.0",
    "@kogito-tooling/kie-editors-standalone": "0.16.0"
  },
  "devDependencies": {
    "@kie-tooling-core/webpack-base": "0.16.0",
    "@kogito-tooling/build-env": "0.16.0",
    "@types/react-router-dom": "^5.1.7",
    "react-router-dom": "^5.2.1"
  }
}
