{
  "name": "@kie-tools-examples/webapp",
  "version": "0.19.2",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack",
    "build:dev": "yarn run run-script-if --bool \"$(build-env global.build.examples)\" --then \"yarn run build --env dev\"",
    "build:prod": "yarn run run-script-if --bool \"$(build-env global.build.examples)\" --then \"yarn run build\"",
    "start": "webpack serve --host 0.0.0.0 --env dev"
  },
  "dependencies": {
    "@kie-tools-core/envelope-bus": "0.19.2",
    "@kie-tools-core/patternfly-base": "0.19.2",
    "@kie-tools-examples/base64png-editor": "0.19.2",
    "@kie-tools-examples/ping-pong-view": "0.19.2",
    "@kie-tools-examples/ping-pong-view-angular": "0.19.2",
    "@kie-tools-examples/ping-pong-view-react": "0.19.2",
    "@kie-tools-examples/todo-list-view": "0.19.2",
    "@kie-tools/kie-bc-editors": "0.19.2",
    "@kie-tools/kie-editors-standalone": "0.19.2"
  },
  "devDependencies": {
    "@kie-tools-core/webpack-base": "0.19.2",
    "@kie-tools/build-env": "0.19.2",
    "@types/react-router-dom": "^5.1.7",
    "react-router-dom": "^5.2.1"
  }
}
