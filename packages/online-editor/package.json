    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@kie-tools-core/run-script-if": "0.21.1",
    "@kie-tools-core/webpack-base": "0.21.1",
    "@kie-tools/build-env": "0.21.1",
    "@kie-tools/extended-services": "0.21.1",
    "@kie-tools/stunner-editors": "0.21.1",
    "@types/history": "^4.7.3",
    "@types/minimatch": "^3.0.4",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "cypress-real-events": "^1.7.0"
  },
  "scripts": {
    "lint": "pnpm run-script-if --bool \"$(build-env global.build.lint)\" --then \"pnpm eslint ./src --ext .ts,.tsx\"",
    "test": "pnpm run-script-if --bool \"$(build-env global.build.test)\" --then \"jest --silent --verbose\"",
    "build:dev": "rimraf dist && webpack --env dev",
    "build:prod": "pnpm lint && pnpm test && rimraf dist && webpack && pnpm test:it",
    "start": "webpack serve --host 0.0.0.0 --env dev",
    "cy:open": "pnpm cypress open --project it-tests --config baseUrl=$(build-env onlineEditor.dev.cypressUrl)",
    "cy:run": "pnpm cypress run --headed -b chrome --project it-tests --config baseUrl=$(build-env onlineEditor.dev.cypressUrl)",
    "test:it:start:extended-services": "npm --prefix ../extended-services run start",
    "test:it": "pnpm run-script-if --bool \"$(build-env global.build.testIT)\" --then  \"pnpm rimraf ./dist-it-tests\" \"pnpm start-server-and-test test:it:start:extended-services http-get://0.0.0.0:21345/ping start https-get://0.0.0.0:$(build-env onlineEditor.dev.port) cy:run\""
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/react"
    ]
  }
}
