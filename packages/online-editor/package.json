    "@kie-tools/form": "0.21.1",
    "@kie-tools/form-dmn": "0.21.1",
    "@kie-tools/i18n-common-dictionary": "0.21.1",
    "@kie-tools/kie-bc-editors": "0.21.1",
    "@kie-tools/kie-sandbox-fs": "0.21.1",
    "@kie-tools/pmml-editor": "0.21.1",
    "@kie-tools/unitables": "0.21.1",
    "@kie-tools/unitables-dmn": "0.21.1",
    "@octokit/plugin-rest-endpoint-methods": "^5.0.1",
    "@octokit/rest": "^18.5.3",
    "@patternfly/react-core": "^4.157.3",
    "@patternfly/react-icons": "^4.11.17",
    "@patternfly/react-tokens": "^4.33.1",
    "buffer": "^6.0.3",
    "history": "^4.9.0",
